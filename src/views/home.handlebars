<h1>List of products</h1>

<div class="container mt-5">

  <div class="row">
    {{#each products}}
      <div class="col-md-4 mb-4">
        <div class="card">
          <div class="card-body">
            <h5 class="card-title">{{this.title}}</h5>
            <p class="card-text">Precio: ${{this.price}}</p>
          </div>
        </div>
      </div>
    {{/each}}
  </div>
</div>